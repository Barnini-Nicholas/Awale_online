<Window x:Class="Awale.View.Historique"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Awale.View"
        mc:Ignorable="d"
        Title="Historique" Height="300" Width="300">

    <ListView HorizontalAlignment="Center" ItemsSource="{Binding ListScores}" Name="invoiceLV" Height="207" VerticalAlignment="Top" >
        <ListView.View>
            <GridView>
                <GridViewColumn Header="J1" DisplayMemberBinding="{Binding J1}" />
                <GridViewColumn Header="Score" DisplayMemberBinding="{Binding ScoreJ1}" />
                <GridViewColumn Header="J2" DisplayMemberBinding="{Binding J2}" />
                <GridViewColumn Header="Score" DisplayMemberBinding="{Binding ScoreJ2}" />
            </GridView>
        </ListView.View>
    </ListView>
</Window>
